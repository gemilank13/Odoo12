<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record model="ir.ui.view" id="inherit_stock_card">
		<field name="name">stock_card.inherit</field>
		<field name="model">stock.card.report.wizard</field>
		<field name="inherit_id" ref="stock_card_report.stock_card_report_wizard_form"/>
		<field name="arch" type="xml">
			<!-- gunakan tag xpath untuk mengacu pada suatu elemen -->
			<xpath expr="//button[@name='button_export_xlsx']" position="after">
				<button name="button_export_xlsx_v2" string="Export XLSX" type="object"/>
			</xpath>

		</field>
	</record>
</odoo>
