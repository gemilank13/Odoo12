<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="kite_penghp_tree" model="ir.ui.view">
		<field name="name">Pengeluaran Hasil Produksi Tree</field>
		<field name="model">djbc.kite_penghp</field>
		<field name="arch" type="xml">
			<tree create="0" edit="0">
				<field name="no_peb"/>
				<field name="tgl_peb"/>
				<field name="mata_uang"/>
				<field name="negara_tujuan"/>
				<field name="no_pengeluaran"/>
				<field name="tgl_pengeluaran"/>
				<field name="kode_barang"/>
				<field name="nama_barang"/>
				<field name="jumlah"/>
				<field name="satuan"/>
				<field name="pembeli"/>
				
				<field name="nilai"/>
				<field name="djbc"/>
				
			</tree>
		</field>
	</record> 



	<record id="kite_penghp_form" model="ir.ui.view">
		<field name="name">Pengeluaran Hasil Produksi Form</field>
		<field name="model">djbc.kite_penghp</field>
		<field name="arch" type="xml">
			<form create="0" edit="0">
				<group>
					<group>
						<field name="no_peb"/>
						<field name="tgl_peb"/>
						<field name="mata_uang"/>
						<field name="negara_tujuan"/>
						<field name="no_pengeluaran"/>
						<field name="tgl_pengeluaran"/>
						
					</group>
					<group>
						<field name="kode_barang"/>
						<field name="nama_barang"/>
						<field name="jumlah"/>
						<field name="satuan"/>
						<field name="pembeli"/>
						
						<field name="nilai"/>
						<field name="djbc"/>
					</group>
				</group>
			</form>
		</field>
	</record>

	<record id="kite_penghp_action" model="ir.actions.act_window">
		<field name="name">Pengeluaran Hasil Produksi</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">djbc.kite_penghp</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree,form</field>
	</record>


</odoo>
