<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="kite_pembb_tree" model="ir.ui.view">
		<field name="name">Pemakaian Bahan Baku Tree</field>
		<field name="model">djbc.kite_pembb</field>
		<field name="arch" type="xml">
			<tree create="0" edit="0">
				<field name="nomor_pengeluaran"/>
				<field name="tanggal_pengeluaran"/>
				<field name="kode_barang"/>
				<field name="nama_barang"/>
				<!-- <field name="jumlah"/> -->
				<field name="satuan"/>
				<!-- <field name="gudang"/> -->
				<field name="jumlah_digunakan"/>
				<field name="jumlah_disubkon"/>
				<field name="penerima_subkon"/>
				<field name="djbc"/>
				
			</tree>
		</field>
	</record> 



	<record id="kite_pembb_form" model="ir.ui.view">
		<field name="name">Pemakaian Bahan Baku Form</field>
		<field name="model">djbc.kite_pembb</field>
		<field name="arch" type="xml">
			<form create="0" edit="0">
				<group>
					<group>
						<field name="nomor_pengeluaran"/>
						<field name="tanggal_pengeluaran"/>
						
					</group>
					<group>
						<field name="kode_barang"/>
						<field name="nama_barang"/>
						<!-- <field name="jumlah"/> -->
						<field name="satuan"/>
						<!-- <field name="gudang"/> -->
						<field name="jumlah_digunakan"/>
						<field name="jumlah_disubkon"/>
						<field name="penerima_subkon"/>
						<field name="djbc"/>
					</group>
				</group>
			</form>
		</field>
	</record>

	<record id="kite_pembb_action" model="ir.actions.act_window">
		<field name="name">Pemakaian Bahan Baku</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">djbc.kite_pembb</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree,form</field>
	</record>


</odoo>
