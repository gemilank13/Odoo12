<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="kite_php_tree" model="ir.ui.view">
		<field name="name">Pemasukan Hasil Produksi Tree</field>
		<field name="model">djbc.kite_php</field>
		<field name="arch" type="xml">
			<tree create="0" edit="0">
				<field name="no_penerimaan"/>
				<field name="tgl_penerimaan"/>
				<field name="kode_barang"/>
				<field name="nama_barang"/>
				<field name="jumlah"/>
				<field name="satuan"/>
				<field name="jumlah_disubkon"/>
				
			</tree>
		</field>
	</record> 



	<record id="kite_php_form" model="ir.ui.view">
		<field name="name">Pemasukan Hasil Produksi Form</field>
		<field name="model">djbc.kite_php</field>
		<field name="arch" type="xml">
			<form create="0" edit="0">
				<group>
					<group>
						<field name="no_penerimaan"/>
						<field name="tgl_penerimaan"/>
						
					</group>
					<group>
						<field name="kode_barang"/>
						<field name="nama_barang"/>
						<field name="jumlah"/>
						<field name="satuan"/>
						<field name="jumlah_disubkon"/>
					</group>
				</group>
			</form>
		</field>
	</record>

	<record id="kite_php_action" model="ir.actions.act_window">
		<field name="name">Pemasukan Hasil Produksi</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">djbc.kite_php</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree,form</field>
	</record>


</odoo>
