<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="kite_mhp_wizard_form" model="ir.ui.view">
        <field name="name">Mutasi Hasil Produksi</field>
        <field name="model">djbc.kite.mhp.wizard</field>
        <field name="arch" type="xml">
		<form>

			<group>
	
				<field name="date_start"/>
				<field name="date_end"/>
				
			</group>
			<footer>
				<button name='generate_laporan' string='Submit' type='object' class='btn-primary'/>
				<button string='Cancel' class='btn-secondary' special='cancel'/>
			</footer>
		</form>
	</field>
    </record>


    <record id="kite_mhp_wizard_action" model="ir.actions.act_window">
        <field name="name">Masukan Periode</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">djbc.kite.mhp.wizard</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref='kite_mhp_wizard_form'/>
        <field name="target">new</field>

    </record>
</odoo>
